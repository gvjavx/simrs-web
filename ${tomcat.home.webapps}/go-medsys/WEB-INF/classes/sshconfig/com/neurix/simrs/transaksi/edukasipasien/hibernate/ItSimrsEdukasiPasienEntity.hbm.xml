<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.edukasipasien.model.ItSimrsEdukasiPasienEntity" dynamic-update="true"
           table="it_simrs_edukasi_pasien_teritegrasi" select-before-update="true" schema="public">

        <id name="idEdukasiPasien">
            <column name="id_edukasi_pasien_teritegrasi" not-null="true" />
        </id>

        <property name="idDetailCheckup">
            <column name="id_detail_checkup"/>
        </property>
        <property name="durasi">
            <column name="durasi"/>
        </property>
        <property name="edukator">
            <column name="edukator"/>
        </property>
        <property name="edukasi">
            <column name="edukasi"/>
        </property>
        <property name="pemahamanAwal">
            <column name="pemahaman_awal"/>
        </property>
        <property name="metodeEdukasi">
            <column name="metode_edukasi"/>
        </property>
        <property name="mediaEdukasi">
            <column name="media_edukasi"/>
        </property>
        <property name="evaluasi">
            <column name="evaluasi"/>
        </property>
        <property name="ttdPasien">
            <column name="ttd_pasien"/>
        </property>
        <property name="ttdStaf">
            <column name="ttd_staf"/>
        </property>
        <property name="tipe">
            <column name="tipe"/>
        </property>
        <property name="keterangan">
            <column name="keterangan"/>
        </property>
        <property name="flag">
            <column name="flag"/>
        </property>
        <property name="action">
            <column name="action"/>
        </property>
        <property name="createdDate">
            <column name="created_date"/>
        </property>
        <property name="createdWho">
            <column name="created_who"/>
        </property>
        <property name="lastUpdate">
            <column name="last_update"/>
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who"/>
        </property>
        <property name="namaTerang">
            <column name="nama_terang"/>
        </property>
        <property name="sip">
            <column name="sip"/>
        </property>
        <property name="namaStaf">
            <column name="nama_staf"/>
        </property>
    </class>
</hibernate-mapping>
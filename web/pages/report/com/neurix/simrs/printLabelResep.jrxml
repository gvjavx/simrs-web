	<queryString>
		<![CDATA[SELECT
a.id_approval_obat,
a.id_detail_checkup,
c.id_pasien,
c.nama,
TO_CHAR(c.tgl_lahir, 'dd-mm-yyyy') as tanggal_lahir,
b.id_obat,
d.nama_obat,
TO_CHAR(e.expired_date, 'dd-mm-yyyy') as expired_date,
CAST('N' AS VARCHAR) as is_racik,
h.tipe_pelayanan,
CASE WHEN h.tipe_pelayanan = 'rawat_inap' THEN
    REPLACE(f.frekuensi,'|', ', ')
ELSE
    CONCAT(REPLACE(split_part(f.frekuensi,'#', 2), '|', ', '), '.')
END as frekuensi,
CASE WHEN h.tipe_pelayanan = 'rawat_inap' THEN
    ''
ELSE
    split_part(f.frekuensi,'#', 3)
END as keterangan_obat,
i.warna_label,
i.warna_background,
i.keterangan,
j.bentuk,
split_part(f.frekuensi,'#', 1) as ketPakai
FROM mt_simrs_permintaan_resep a
INNER JOIN (
	SELECT
	id_transaksi_obat_detail,
	id_approval_obat,
	id_obat,
	id_racik,
	nama_racik
	FROM mt_simrs_transaksi_obat_detail
	WHERE id_racik IS NULL
) b ON a.id_approval_obat = b.id_approval_obat
INNER JOIN im_simrs_pasien c ON a.id_pasien = c.id_pasien
INNER JOIN im_simrs_header_obat d ON b.id_obat = d.id_obat
INNER JOIN (
	SELECT
	id_transaksi_obat_detail,
	expired_date
	FROM mt_simrs_transaksi_obat_detail_batch
	GROUP BY id_transaksi_obat_detail, expired_date
) e ON b.id_transaksi_obat_detail = e.id_transaksi_obat_detail
INNER JOIN mt_simrs_transaksi_obat_detail f ON d.id_obat = f.id_obat AND f.id_approval_obat = a.id_approval_obat AND f.id_racik IS NULL
INNER JOIN it_simrs_header_detail_checkup g ON a.id_detail_checkup = g.id_detail_checkup
INNER JOIN (
  SELECT
  a.id_pelayanan,
  b.tipe_pelayanan
  FROM im_simrs_pelayanan a
  INNER JOIN im_simrs_header_pelayanan b ON a.id_header_pelayanan = b.id_header_pelayanan
) h ON g.id_pelayanan = h.id_pelayanan
LEFT JOIN (
	SELECT
	a.keterangan,
	a.warna_label,
	a.warna_background,
	b.id_obat,
	b.keterangan_lain
	FROM im_simrs_keterangan_obat a
	INNER JOIN it_simrs_keterangan_resep b ON a.id = b.id_keterangan_obat
	WHERE b.id_permintaan_resep = $P{reportParams.id}
) i ON b.id_obat = i.id_obat
LEFT JOIN im_simrs_bentuk_barang j ON d.id_bentuk = j.id_bentuk
WHERE a.id_permintaan_resep = $P{reportParams.id}
UNION ALL
SELECT
a.id_approval_obat,
a.id_detail_checkup,
c.id_pasien,
c.nama,
TO_CHAR(c.tgl_lahir, 'dd-mm-yyyy') as tanggal_lahir,
d.id,
d.nama as keterangan,
CAST('' AS VARCHAR) as expired_date,
CAST('Y' AS VARCHAR) as is_racik,
h.tipe_pelayanan,
CASE WHEN h.tipe_pelayanan = 'rawat_inap' THEN
    REPLACE(d.frekuensi,'|', ', ')
ELSE
    CONCAT(REPLACE(split_part(d.frekuensi,'#', 2), '|', ', '), '.')
END as frekuensi,
CASE WHEN h.tipe_pelayanan = 'rawat_inap' THEN
    ''
ELSE
    split_part(d.frekuensi,'#', 3)
END as keterangan_obat,
CAST('' AS VARCHAR) as warna_label,
CAST('' AS VARCHAR) as  warna_background,
CAST('Racik' AS VARCHAR) as  keterangan,
d.kemasan as  bentuk,
split_part(d.frekuensi,'#', 1) as ketPakai
FROM mt_simrs_permintaan_resep a
INNER JOIN (
	SELECT
	id_approval_obat,
	id_racik
	FROM mt_simrs_transaksi_obat_detail
	WHERE id_racik IS NOT NULL
	GROUP BY id_approval_obat,
	id_racik
) b ON a.id_approval_obat = b.id_approval_obat
INNER JOIN im_simrs_pasien c ON a.id_pasien = c.id_pasien
INNER JOIN it_simrs_obat_racik d ON b.id_racik = d.id
INNER JOIN it_simrs_header_detail_checkup g ON a.id_detail_checkup = g.id_detail_checkup
INNER JOIN (
  SELECT
  a.id_pelayanan,
  b.tipe_pelayanan
  FROM im_simrs_pelayanan a
  INNER JOIN im_simrs_header_pelayanan b ON a.id_header_pelayanan = b.id_header_pelayanan
) h ON g.id_pelayanan = h.id_pelayanan
WHERE a.id_permintaan_resep = $P{reportParams.id}]]>
	</queryString>

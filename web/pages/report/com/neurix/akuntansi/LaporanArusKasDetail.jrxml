	<queryString>
		<![CDATA[SELECT
  a.*,
  a.lastsaldo + a.penerimaan - a.pengeluaran as saldosekarang
FROM
  (
    SELECT
      distinct a.kode_rekening as kodeRekening,
      a.grup as grup,
      $P{periode} as periode,
      a.nama_kode_rekening as namaRekening,
      c.jumlah_debit as penerimaan,
      c.jumlah_kredit as pengeluaran,
	  c.no_jurnal as nojurnal,
	  c.keterangan as keterangan,
	  c.tanggal_jurnal as tanggaljurnal,
      (
        CASE WHEN length(a.kode_rekening) = 12 THEN (
          CASE WHEN d.jumlah_debit IS NULL
          OR d.jumlah_kredit IS NULL THEN 0 ELSE (d.jumlah_debit - d.jumlah_kredit) END
        ) ELSE NULL END
      ) as lastSaldo
    FROM
      (
        SELECT
          x.*,
          y.nama_kode_rekening as grup
        FROM
          (
            select
              kode_rekening,
              nama_kode_rekening,
              rekening_id,
              left(kode_rekening, 9) as grup_id
            from
              im_akun_kode_rekening
            where
              rekening_id IN (
                select
                  rekening_id
                from
                  im_akun_report_detail
                where
                  report_id = $P{reportId}
                  and flag = 'Y'
              )
            group by
              kode_rekening,
              nama_kode_rekening,
              rekening_id
            order by
              kode_rekening
          ) x
          INNER JOIN im_akun_kode_rekening y ON x.grup_id = y.kode_rekening
      ) a
      left outer JOIN (
        select
          y.rekening_id as rekening_id,
		  x.no_jurnal as no_jurnal,
		  x.tanggal_jurnal as tanggal_jurnal,
		  x.keterangan as keterangan,
          sum(y.jumlah_debit * x.kurs) as jumlah_debit,
          sum(y.jumlah_kredit * x.kurs) as jumlah_kredit
        from
          it_akun_jurnal x
          inner join it_akun_jurnal_detail y on x.no_jurnal = y.no_jurnal
          left outer join im_akun_mata_uang mu ON (x.mata_uang_id = mu.mata_uang_id)
        where
          to_char(x.tanggal_jurnal, 'dd-MM-yyyy') = $P{periode}
          and x.registered_flag = 'Y'
          and x.branch_id IN ($P!{branchId})
        group by
          y.rekening_id,
		  x.no_jurnal,
		  x.tanggal_jurnal,
		  x.keterangan
      ) c ON c.rekening_id = a.rekening_id
      left outer JOIN (
        select
          y.rekening_id as rekening_id,
		  x.no_jurnal,
          sum(y.jumlah_debit * x.kurs) as jumlah_debit,
          sum(y.jumlah_kredit * x.kurs) as jumlah_kredit
        from
          it_akun_jurnal x
          inner join it_akun_jurnal_detail y on x.no_jurnal = y.no_jurnal
        where
          x.tanggal_jurnal < (
            select
              to_date($P{periode}, 'dd-MM-yyyy')
          )
          and x.branch_id IN ($P!{branchId})
        group by
          y.rekening_id,
		  x.no_jurnal
      ) d ON d.rekening_id = a.rekening_id
    WHERE
      c.jumlah_debit IS NOT NULL
  ) a]]>

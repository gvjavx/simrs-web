	<queryString>
		<![CDATA[select grup.nama_kode_rekening as grup,mainValue.* from (
select coalesce(saldo.saldo, 0)               saldo_lalu,
       rekening.kode_rekening,
       rekening.nama_kode_rekening,
       coalesce(sum(detail.jumlah_debit), 0)  debit,
       coalesce(sum(detail.jumlah_kredit), 0) kredit
from it_akun_jurnal_detail detail
       join it_akun_jurnal jurnal on jurnal.no_jurnal = detail.no_jurnal
                                       and registered_flag = 'Y'
                                       and to_char(jurnal.registered_date, 'MM-yyyy') = $P{reportParams.periode}
                                       and jurnal.branch_id in ($P!{reportParams.branchId})
       right join (select kodering.kode_rekening, kodering.nama_kode_rekening
                   from im_akun_report_detail reportSetting
                          join im_akun_kode_rekening kodering on kodering.rekening_id = reportSetting.rekening_id
                   where reportSetting.report_id = $P{reportParams.reportId}
                     and reportSetting.flag = 'Y') rekening
         on detail.nomor_rekening ilike concat(rekening.kode_rekening, '%')
       left outer join (select left(kode_rekening, 3)                 kode_rekening,
                               sum(jumlah_debit)                      debit,
                               sum(jumlah_kredit)                     kredit,
                               sum(jumlah_debit) - sum(jumlah_kredit) saldo
                        from it_akun_saldo_akhir
                        where periode = $P{periodeLalu}
                          and branch_id in ($P!{reportParams.branchId})
                          and kode_rekening in (select rekening.kode_rekening
                                                from im_akun_report_detail reportDetail
                                                       join im_akun_kode_rekening rekening
                                                         on rekening.rekening_id = reportDetail.rekening_id
                                                where report_id = $P{reportParams.reportId}
                                                  and reportDetail.flag = 'Y')
                        group by left(kode_rekening, 3)) saldo on saldo.kode_rekening = rekening.kode_rekening
group by rekening.kode_rekening, rekening.nama_kode_rekening, saldo.saldo) mainValue
join im_akun_kode_rekening grup on grup.kode_rekening = left(mainValue.kode_rekening,1)
order by mainValue.kode_rekening]]>
	</queryString>

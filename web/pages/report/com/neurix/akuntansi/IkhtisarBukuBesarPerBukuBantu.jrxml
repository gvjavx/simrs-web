	<queryString>
		<![CDATA[SELECT
  distinct a.nama_kode_rekening as namaRekening,
  a.kode_rekening as koderekening,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN c.jumlah_debit ELSE NULL END
  ) as lastdebit,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN c.jumlah_kredit ELSE NULL END
  ) as lastkredit,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN c.saldo ELSE NULL END
  ) as lastsaldo,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN c.posisi ELSE NULL END
  ) as lastposisi,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN b.jumlah_debit ELSE NULL END
  ) as jumlahdebit,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN b.jumlah_kredit ELSE NULL END
  ) as jumlahkredit,
  '01-2010' as periode,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN (
      CASE WHEN c.jumlah_debit IS NULL THEN 0 ELSE c.jumlah_debit END
    ) + (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    ) ELSE NULL END
  ) as currdebit,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN (
      CASE WHEN c.jumlah_kredit IS NULL THEN 0 ELSE c.jumlah_kredit END
    ) + (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    ) ELSE NULL END
  ) as currkredit,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN CASE WHEN c.posisi = 'K' THEN abs(
      (
        (
          CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
        ) + (
          CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
        )
      ) - (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) ELSE abs(
      (
        (
          CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
        ) + (
          CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
        )
      ) - (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) END ELSE NULL END
  ) as currsaldo,
  (
    CASE WHEN length(a.kode_rekening) = 6 THEN CASE WHEN c.posisi = 'K' THEN CASE WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) - (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    ) > 0 THEN 'K' WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) - (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    ) < 0 THEN 'D' ELSE NULL END ELSE CASE WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) - (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    ) < 0 THEN 'K' WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) - (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    ) > 0 THEN 'D' ELSE NULL END END ELSE NULL END
  ) as currposisi
FROM
  (
    select
      kode_rekening,
      nama_kode_rekening
    from
      im_akun_kode_rekening
    where
      length(kode_rekening) >= 3
      and length(kode_rekening) <= 6
    group by
      kode_rekening,
      nama_kode_rekening
    order by
      kode_rekening
  ) a
  LEFT OUTER JOIN (
    select
      substr(y.kode_rekening, 1, 6) as kode_rekening,
      sum(y.jumlah_debit * x.kurs) as jumlah_debit,
      sum(y.jumlah_kredit * x.kurs) as jumlah_kredit,
      x.cabang_id
    from
      t_jurnal x
      inner join t_jurnal_detail y on x.no_jurnal = y.no_jurnal
    where
      to_char(x.created_date, 'MM-yyyy') = $P{reportParams.periode}  and
      cabang_id = $P{reportParams.cabangId}
    group by
      substr(y.kode_rekening, 1, 6),
      cabang_id
  ) b ON b.kode_rekening = a.kode_rekening
  LEFT OUTER JOIN (
    select
      kode_rekening,
      jumlah_debit,
      jumlah_kredit,
      saldo,
      posisi
    from
      t_kode_rekening_saldo
    where
      to_date(periode, 'MM-yyyy') = to_date($P{reportParams.periode}, 'MM-yyyy') - Interval '1 month' and cabang_id= $P{reportParams.cabangId}
  ) c ON a.kode_rekening = c.kode_rekening
order by
  a.kode_rekening]]>

	<queryString>
		<![CDATA[select kk.*, grup4.kode_rekening kodering4, grup4.nama_kode_rekening namaKodering4 from (
              select ss.*, grup3.kode_rekening kodering3, grup3.nama_kode_rekening namakodering3
from (select mainValue.*, grup2.kode_rekening kodering2, grup2.nama_kode_rekening namakodering2
      from (select coalesce(saldo.debit, 0) - coalesce(saldo.kredit, 0) saldo_lalu,
                   rekening.kode_rekening,
                   rekening.nama_kode_rekening,
                   sum(coalesce(detail.jumlah_debit, 0))                debit,
                   sum(coalesce(detail.jumlah_kredit, 0))               kredit
            from it_akun_jurnal_detail detail
                   join it_akun_jurnal jurnal on jurnal.no_jurnal = detail.no_jurnal
                                                   and registered_flag = 'Y'
                                                   and to_char(jurnal.registered_date, 'MM-yyyy') = $P{periode}
                                                   and jurnal.branch_id in ($P!{branchId})
                   right join (select kodering.kode_rekening, kodering.nama_kode_rekening
                               from im_akun_report_detail reportSetting
                                      join im_akun_kode_rekening kodering
                                        on kodering.rekening_id = reportSetting.rekening_id
                               where reportSetting.report_id = $P{reportId}
                                 and reportSetting.flag = 'Y'
                                 and length(kode_rekening) = 12) rekening
                     on detail.nomor_rekening ilike concat(rekening.kode_rekening, '%')
                   left outer join (select sum(
                                             coalesce(saldo.jumlah_debit, 0) - coalesce(saldo.jumlah_kredit, 0)) saldo,
                                           rekening.kode_rekening,
                                           rekening.nama_kode_rekening,
                                           sum(coalesce(saldo.jumlah_debit, 0))                                  debit,
                                           sum(coalesce(saldo.jumlah_kredit, 0))                                 kredit
                                    from it_akun_saldo_akhir saldo
                                           right join (select kodering.kode_rekening, kodering.nama_kode_rekening
                                                       from im_akun_report_detail reportSetting
                                                              join im_akun_kode_rekening kodering
                                                                on kodering.rekening_id = reportSetting.rekening_id
                                                       where reportSetting.report_id = $P{reportId}
                                                         and reportSetting.flag = 'Y') rekening
                                             on saldo.rekening_id = rekening.kode_rekening
                                    where saldo.periode = $P{periodeLalu}
                                      and saldo.branch_id in ($P!{branchId})
                                    group by rekening.kode_rekening, rekening.nama_kode_rekening) saldo
                     on saldo.kode_rekening = rekening.kode_rekening
            group by rekening.kode_rekening, rekening.nama_kode_rekening, saldo.saldo, saldo.debit,
                     saldo.kredit) as mainValue
             join im_akun_kode_rekening grup2 on grup2.kode_rekening like left(mainValue.kode_rekening, 3)) as ss
       join im_akun_kode_rekening grup3
         on grup3.kode_rekening like left(ss.kode_rekening, 6) and length(grup3.kode_rekening) = 6 )as kk
join im_akun_kode_rekening grup4 on grup4.kode_rekening like left(kk.kode_rekening, 9) and length(grup4.kode_rekening) = 9]]>
	</queryString>

	<parameter name="periode" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["05-2008"]]></defaultValueExpression>
	</parameter>
	<parameter name="kode_rekening" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["103.03.00.00"]]></defaultValueExpression>
	</parameter>
	<parameter name="nomor_master" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["40023"]]></defaultValueExpression>
	</parameter>
	<parameter name="rekening_id" class="java.lang.String"/>
	<parameter name="branch_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
    (CASE WHEN sum(a.jumlah_debit) IS NULL THEN 0.0 ELSE sum(a.jumlah_debit) END) as debit,
    (CASE WHEN sum(a.jumlah_kredit) IS NULL THEN 0.0 ELSE sum(a.jumlah_kredit) END) as kredit
from
	it_akun_saldo_akhir a
where
          to_date(a.periode, 'MM-yyyy') = to_date($P{periode}, 'MM-yyyy') - Interval '1 month'
    and a.rekening_id = $P{rekening_id}
    and a.master_id = $P{nomor_master}
    and a.branch_id IN ( $P!{branch_id} )]]>
	</queryString>
	<field name="debit" class="java.math.BigDecimal"/>
	<field name="kredit" class="java.math.BigDecimal"/>
	<variable name="debit_awal" class="java.math.BigDecimal">

	<queryString>
		<![CDATA[select pegawai.nama_pegawai,
       last_shift.shift_name,
       jurnal.no_jurnal,
       jurnaldetail.no_nota,
       kodering.kode_rekening,
       kodering.nama_kode_rekening,
       jurnal.keterangan,
       jurnaldetail.jumlah_debit,
       jurnaldetail.jumlah_kredit,
       jurnal.branch_id,
       jurnal.registered_date,
       jurnal.metode_bayar,
       jurnal.registered_user_id,
cast(jurnal.last_update as timestamp),
       last_shift.shift_ke,
       last_shift.shift_end,
       last_shift.tipe_shift_kasir,
       last_shift.shift_start
from it_akun_jurnal_detail jurnaldetail
            join it_akun_jurnal jurnal on jurnal.no_jurnal = jurnaldetail.no_jurnal
            join im_akun_kode_rekening kodering on jurnaldetail.nomor_rekening = kodering.kode_rekening
            join im_hris_pegawai pegawai on pegawai.nip = jurnal.registered_user_id
            join (
                 select ROW_NUMBER() OVER (ORDER BY cast(shiftKerja.tanggal ||' '|| shift.jam_akhir as timestamp) ) shift_ke,
                        shiftDetail.jadwal_shift_kerja_detail_id,
                        shiftDetail.nip,
                        shiftDetail.nama_pegawai,
                        shift.shift_name,
                        cast(shiftKerja.tanggal ||' '|| shift.jam_awal as timestamp)  as shift_start,
                        cast(shiftKerja.tanggal ||' '|| shift.jam_akhir as timestamp) as shift_end,
                        shift.tipe_shift_kasir
                 from im_hris_pegawai pegawai
                             join it_hris_jadwal_shift_kerja_detail shiftDetail on shiftDetail.nip = pegawai.nip
                             join it_hris_jadwal_shift_kerja shiftKerja
                          on shiftKerja.jadwal_shift_kerja_id = shiftDetail.jadwal_shift_kerja_id
                             join im_hris_shift shift on shift.shift_id = shiftDetail.shift_id
                 where shiftDetail.profesi_id = (
                                                select jamShift.profesi_id
                                                from it_hris_jadwal_shift_kerja_detail shiftdetail
                                                            join it_hris_jadwal_shift_kerja shift on shiftdetail.jadwal_shift_kerja_id = shift.jadwal_shift_kerja_id
                                                            join im_hris_shift jamShift on jamShift.shift_id = shiftdetail.shift_id
                                                where nip = $P{userId}
                     and shift.tanggal = $P{tanggal}
                 )
    and shift.tipe_shift_kasir = $P{tipeKasir}
    and shiftKerja.tanggal = $P{tanggal}
    ) as last_shift on last_shift.nip = jurnal.registered_user_id
    where jurnal.registered_date = $P{tanggal}
    and jurnaldetail.nomor_rekening in ( select kodering.kode_rekening from im_akun_report_detail reportSetting
    join  im_akun_kode_rekening kodering on kodering.rekening_id = reportSetting.rekening_id
    where reportSetting.report_id = $P{reportId} and reportSetting.flag = 'Y'
    )
    order by last_shift.shift_start]]>
	</queryString>

	<queryString>
		<![CDATA[select u.kode_rekening,
       u.nama    as            group,
       periode,
       kd_barang,
       master_id as            nomor_master,
       u.nama_kode_rekening,
       keterangan,
       no_jurnal,
       no_nota,
       tanggal_jurnal,
       coalesce(debitnow, 0)   debitnow,
       coalesce(kreditnow, 0)  kreditnow,
       coalesce(pastdebit, 0)  pastdebit,
       coalesce(pastkredit, 0) pastkredit
from (select y.master_id,
             kd_barang,
             m.nama,
             $P{periode}                  periode,
             x.keterangan,
             x.no_jurnal,
             y.no_nota,
             y.nomor_rekening as        kode_rekening,
             coalesce(jumlah_debit, 0)  debitnow,
             coalesce(jumlah_kredit, 0) kreditnow,
             null                       pastdebit,
             null                       pastkredit,
             kodering.nama_kode_rekening,
             x.tanggal_jurnal

      from it_akun_jurnal x
             inner join it_akun_jurnal_detail y on x.no_jurnal = y.no_jurnal
             join im_akun_kode_rekening kodering on kodering.kode_rekening = y.nomor_rekening
             left join im_akun_master m on m.nomor_master = y.master_id
             left join im_akun_mata_uang mu ON (x.mata_uang_id = mu.mata_uang_id)
      where to_char(x.tanggal_jurnal, 'MM-yyyy') = $P{periode}
        and x.registered_flag = 'Y'
        and x.branch_id IN ($P!{reportParams.branchId}) and kode_rekening = $P{kode_rekening}
and ( kd_barang ilike $P{kd_barang} and master_id ilike $P{vendor_id} )
      union all
      select d.master_id,
             d.kd_barang,
             m.nama,
             a.periode                    periode,
             ''                           keterangan,
             null                           no_jurnal,
             ''                           no_nota,
             d.kode_rekening,
             coalesce(null, 0)            debitnow,
             coalesce(null, 0)            kreditnow,
             coalesce(d.jumlah_debit, 0)  pastdebit,
             coalesce(d.jumlah_kredit, 0) pastkredit,
             ''                           nama_kode_rekening,
             null                         tanggal_jurnal
      from it_akun_saldo_akhir a
             join it_akun_saldo_akhir_detail d on a.saldo_akhir_id = d.saldo_akhir_id
             left join im_akun_master m on m.nomor_master = d.master_id
     where to_date(a.periode,'MM-yyyy') = to_date($P{periode}, 'MM-yyyy') - Interval '1 month'
     and a.branch_id in ($P!{reportParams.branchId}) and a.kode_rekening = $P{kode_rekening}
and ( kd_barang ilike $P{kd_barang} and d.master_id ilike $P{vendor_id} )
)  u order by u.no_jurnal]]>
	</queryString>

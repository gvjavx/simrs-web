	<parameter name="nomor_master" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["40023"]]></defaultValueExpression>
	</parameter>
	<parameter name="rekening_id" class="java.lang.String"/>
	<parameter name="branch_id" class="java.lang.String"/>
	<parameter name="tanggal_awal" class="java.sql.Date"/>
	<parameter name="tanggal_akhir" class="java.sql.Date"/>
	<parameter name="periode" class="java.lang.String"/>
	<parameter name="tanggal_awal_bulan" class="java.sql.Date"/>
	<queryString>
		<![CDATA[SELECT
	debitBulanLalu+debitTangalSekarang as debit,
	kreditBulanLalu+kreditTangalSekarang as kredit
FROM (
	select
	(CASE WHEN sum(sa.jumlah_debit) IS NULL THEN 0.0 ELSE sum(sa.jumlah_debit) END) as debitBulanLalu,
    (CASE WHEN sum(sa.jumlah_kredit) IS NULL THEN 0.0 ELSE sum(sa.jumlah_kredit) END) as kreditBulanLalu,
    (CASE WHEN sum(a.jumlah_debit) IS NULL THEN 0.0 ELSE sum(a.jumlah_debit) END) as debitTangalSekarang,
    (CASE WHEN sum(a.jumlah_kredit) IS NULL THEN 0.0 ELSE sum(a.jumlah_kredit) END) as kreditTangalSekarang
FROM
it_akun_jurnal_detail a
LEFT JOIN it_akun_saldo_akhir sa ON a.rekening_id = sa.rekening_id
LEFT JOIN it_akun_jurnal b on a.no_jurnal = b.no_jurnal
where
    b.tanggal_jurnal <= $P{tanggal_awal}
	AND b.tanggal_jurnal >= $P{tanggal_awal_bulan}
	AND to_date(sa.periode, 'MM-yyyy') = to_date($P{periode}, 'MM-yyyy') - Interval '1 month'
    and sa.rekening_id = $P{rekening_id}
    and b.registered_flag = 'Y'
    and b.branch_id IN ($P!{branch_id})
) a]]>
	</queryString>
	<field name="debit" class="java.math.BigDecimal"/>
	<field name="kredit" class="java.math.BigDecimal"/>
	<variable name="debit_awal" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{debit}]]></variableExpression>
	</variable>
	<variable name="kredit_awal" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{kredit}]]></variableExpression>
	</variable>

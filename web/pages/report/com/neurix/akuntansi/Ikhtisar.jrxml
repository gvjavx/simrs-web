	<queryString>
		<![CDATA[SELECT
  a.kode_rekening as koderekening,
  a.nama_kode_rekening as namarekening,
  b.nomor_master as nomorMaster,
  b.nama as namaMaster,
  c.jumlah_debit as lastdebit,
  c.jumlah_kredit as lastkredit,
  c.saldo as lastsaldo,
  c.posisi as lastposisi,
  b.jumlah_debit as jumlahdebit,
  b.jumlah_kredit as jumlahkredit,
  (
    (
      CASE WHEN c.jumlah_debit IS NULL THEN 0 ELSE c.jumlah_debit END
    ) + (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    )
  ) as currdebit,
  (
    (
      CASE WHEN c.jumlah_kredit IS NULL THEN 0 ELSE c.jumlah_kredit END
    ) + (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    )
  ) as currkredit,
  (
    CASE WHEN c.posisi = 'K' THEN abs(
      (
        (
          CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
        ) + (
          CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
        )
      ) - (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) ELSE (
        (
          CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
        ) + (
          CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
        )
      ) - (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      ) END
  ) as currsaldo,
  (
    CASE WHEN c.posisi = 'K' THEN CASE WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) - (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    ) > 0 THEN 'K' WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) - (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    ) < 0 THEN 'D' ELSE NULL END ELSE CASE WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) - (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    ) < 0 THEN 'K' WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) - (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    ) > 0 THEN 'D' ELSE NULL END END
  ) as currposisi,
  $P{reportParams.periode} as periode
FROM
  (
    select
      rekening_id,
      kode_rekening,
      nama_kode_rekening
    from
      im_akun_kode_rekening
    where
      rekening_id IN (
        SELECT
          rekening_id
        FROM
          im_akun_report_detail
        WHERE
          report_id = $P{reportParams.reportId}
	and tipe_laporan=$P{reportParams.tipeLaporan}
	and flag='Y'
      )
    group by
      rekening_id,
      kode_rekening,
      nama_kode_rekening
    order by
      kode_rekening
  ) a
  INNER JOIN (
    select
      y.rekening_id as rekening_id,
      y.master_id as nomor_master,
      z.nama as nama,
      sum(y.jumlah_debit * x.kurs) as jumlah_debit,
      sum(y.jumlah_kredit * x.kurs) as jumlah_kredit
    from
      it_akun_jurnal x
      inner join it_akun_jurnal_detail y on x.no_jurnal = y.no_jurnal
      inner join im_akun_master z on y.master_id = z.nomor_master
    where
      to_char(x.tanggal_jurnal, 'MM-yyyy') =  $P{reportParams.periode}
      and x.branch_id IN  ($P!{reportParams.branchId})
    group by
      y.rekening_id,
      y.master_id,
      z.nama
  ) b ON b.rekening_id = a.rekening_id
  LEFT OUTER JOIN (
    select
      rekening_id,
      master_id,
      jumlah_debit,
      jumlah_kredit,
      saldo,
      posisi
    from
      it_akun_saldo_akhir_detail
    where
      to_date(periode, 'MM-yyyy') = to_date($P{reportParams.periode}, 'MM-yyyy') - Interval '1 month'
      and branch_id IN ($P!{reportParams.branchId})
  ) c ON a.rekening_id = c.rekening_id
  and c.master_id = b.nomor_master
ORDER BY
  koderekening]]>
	</queryString>

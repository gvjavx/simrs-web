	<queryString>
		<![CDATA[select
	a.*,
	b.kode_rekening,
	b.nama_kode_rekening,
	c.nama
from
	it_akun_jurnal_detail a LEFT JOIN
	im_akun_kode_rekening b ON a.nomor_rekening=b.kode_rekening LEFT JOIN
	( select DISTINCT nomor_master,nama from im_akun_master) c ON c.nomor_master=a.master_id
where
	no_jurnal=$P{noJurnal} AND
	jumlah_kredit <> 0
ORDER BY jumlah_debit DESC]]>
	</queryString>

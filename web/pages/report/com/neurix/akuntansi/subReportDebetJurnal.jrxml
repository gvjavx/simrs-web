	<queryString>
		<![CDATA[select
	a.*,
	b.kode_rekening,
	c.nama
from
	it_akun_pembayaran_utang_piutang_detail a LEFT JOIN
	im_akun_kode_rekening b ON a.rekening_id=b.rekening_id LEFT JOIN
	( select DISTINCT nomor_master,nama from im_akun_master) c ON c.nomor_master=a.master_id
where
	pembayaran_utang_piutang_id=$P{pembayaranId}
ORDER BY debit desc]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
	peg.nama_pegawai,
	mts.mutasi_id,
	mts.no_sk,
	TO_CHAR(mts.tanggal_efektif, 'DD-MM-YYYY') tanggal_efektif,
	mts.nip,
	br1.branch_name branch_lama,
	dep1.department_name divisi_lama,
	pos1.position_name posisi_lama,
	br2.branch_name branch_baru,
	dep2.department_name divisi_baru,
	pos2.position_name posisi_baru
FROM it_hris_mutasi_jabatan mts
JOIN im_hris_pegawai peg
ON peg.nip = mts.nip
JOIN im_branches br1
ON br1.branch_id = mts.branch_lama_id
JOIN im_branches br2
ON br2.branch_id = mts.branch_baru_id
JOIN im_hris_department dep1
ON dep1.department_id = mts.divisi_lama_id
JOIN im_hris_department dep2
ON dep2.department_id = mts.divisi_baru_id
JOIN im_position pos1
ON pos1.position_id = mts.position_lama_id
JOIN im_position pos2
ON pos2.position_id = mts.position_lama_id
WHERE mutasi_id = $P{reportParams.mutasiId};]]>
	</queryString>

	<queryString>
		<![CDATA[select
	pegawai.*,
	case when no_telp is null then '-' else no_telp end as no_telp,
	case when cabang_bank is null then '-' else cabang_bank end as cabang_bank2,
	case when jenis_kelamin = 'L' then 'Laki-laki' else 'Perempuan' end as gender,
	case when to_char(pegawai.tanggal_menikah, 'DD-MM-YYYY') is null then '-' else to_char(pegawai.tanggal_menikah, 'DD-MM-YYYY') end as menikah,
	upper(nama_bank) as bank_name,
	im_posisi.position_name,
	bagian.nama_bagian,
	golongan.golongan_name,
	case when dapen.dana_pensiun is null then '-' else dapen.dana_pensiun end as dapen_name,
	case when study.type_study is null then '-' else study.type_study end as type_study,
	case when study.program_study is null then '' when study.program_study = '' then '' else study.program_study end,
	branch.branch_name
from
	im_hris_pegawai pegawai
	left join it_hris_pegawai_position posisi on posisi.nip = pegawai.nip and posisi.flag = 'Y'
	left join im_position im_posisi on im_posisi.position_id = posisi.position_id
	left join im_hris_position_bagian bagian on bagian.bagian_id = im_posisi.bagian_id
	left join im_hris_golongan golongan on golongan.golongan_id = pegawai.golongan_id
	left join im_hris_payroll_dana_pensiun dapen on dapen.dana_pensiun_id = pegawai.dana_pensiun
	left join im_hris_study study on study.nip = pegawai.nip
	left join im_branches branch on branch.branch_id = posisi.branch_id
where
	pegawai.nip = $P{reportParams.nip}
order by
	study.tahun_akhir desc
limit 1]]>
	</queryString>

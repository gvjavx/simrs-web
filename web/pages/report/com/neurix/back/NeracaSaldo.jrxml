	<queryString>
		<![CDATA[SELECT
  distinct
  a.grup as grup,
  a.nama_kode_rekening as namaRekening,
  a.kode_rekening as kodeRekening,
  c.jumlah_debit as lastDebit,
  c.jumlah_kredit as lastKredit,
  c.saldo as lastSaldo,
  c.posisi as lastPosisi,
  b.jumlah_debit as jumlahDebit,
  b.jumlah_kredit as jumlahKredit,
  $P{reportParams.periode} as periode,
  (
    (
      CASE WHEN c.jumlah_debit IS NULL THEN 0 ELSE c.jumlah_debit END
    ) + (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    )
  ) as currDebit,
  (
    (
      CASE WHEN c.jumlah_kredit IS NULL THEN 0 ELSE c.jumlah_kredit END
    ) + (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    )
  ) as currKredit,
  (
    CASE WHEN c.posisi = 'K' THEN abs(
      (
        (
          CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
        ) + (
          CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
        )
      ) - (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) ELSE abs(
      (
        (
          CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
        ) + (
          CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
        )
      ) - (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) END
  ) as currSaldo,
  (
    CASE WHEN c.posisi = 'K' THEN CASE WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) - (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    ) > 0 THEN 'K' WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
      )
    ) - (
      CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
    ) < 0 THEN 'D' ELSE NULL END ELSE CASE WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) - (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    ) < 0 THEN 'K' WHEN (
      (
        CASE WHEN c.saldo IS NULL THEN 0 ELSE c.saldo END
      ) + (
        CASE WHEN b.jumlah_debit IS NULL THEN 0 ELSE b.jumlah_debit END
      )
    ) - (
      CASE WHEN b.jumlah_kredit IS NULL THEN 0 ELSE b.jumlah_kredit END
    ) > 0 THEN 'D' ELSE NULL END END
  ) as currPosisi
FROM
  (
    SELECT
      rek.*,
      kr.nama_kode_rekening as grup
    FROM
      (
        select
          kode.kode_rekening,
          kode.rekening_id,
          kode.nama_kode_rekening,
          LEFT(kode_rekening, 1) as grup_id
        from
          im_akun_kode_rekening kode
          INNER JOIN im_akun_report_detail rd ON rd.rekening_id = kode.rekening_id
        where
          rd.report_id = 'RPT01'
        group by
          kode_rekening,
          kode.nama_kode_rekening,
          kode.rekening_id
        order by
          kode_rekening
      ) rek
      INNER JOIN im_akun_kode_rekening kr ON rek.grup_id = kr.kode_rekening
  ) a
  LEFT OUTER JOIN (
    select
      rekening_id_parent as rekening_id_parent,
      sum(jumlah_debit) as jumlah_debit,
      sum(jumlah_kredit) as jumlah_kredit,
      branch_id as branch_id
    from
      (
        select
          y.rekening_id as rekening_id,
          rek.rekening_id_parent as rekening_id_parent,
		  (y.jumlah_debit * x.kurs) as jumlah_debit,
          (y.jumlah_kredit * x.kurs) as jumlah_kredit,
          x.created_date as created_date,
          x.registered_flag as registered_flag,
          x.branch_id
        from
          t_jurnal x
          INNER JOIN t_jurnal_detail y on x.no_jurnal = y.no_jurnal
          INNER JOIN (
            SELECT
              rekening_id_parent,
              rekening_id
            FROM
              im_akun_kode_rekening kr
              INNER JOIN (
                SELECT
                  kode_rekening as kode_rekening_parent,
                  rd.rekening_id as rekening_id_parent
                FROM
                  im_akun_report_detail rd
                  INNER JOIN im_akun_kode_rekening kr ON rd.rekening_id = kr.rekening_id
                WHERE
                  report_id = 'RPT01'
              ) a ON kr.kode_rekening ilike CONCAT(a.kode_rekening_parent, '%')
          ) rek ON rek.rekening_id = y.rekening_id
        where
          to_char(x.tanggal_jurnal, 'MM-yyyy') = $P{reportParams.periode}
          and x.registered_flag = 'Y'
          and x.flag = 'Y'
          AND y.rekening_id IN (rek.rekening_id)
      ) k
    where
      branch_id = $P{reportParams.branchId}
    group by
      rekening_id_parent,
      branch_id
  ) b ON b.rekening_id_parent = a.rekening_id
  LEFT OUTER JOIN (
    select
      *
    from
      it_akun_saldo_akhir
    where
      to_date(periode, 'MM-yyyy') = to_date($P{reportParams.periode}, 'MM-yyyy') - Interval '1 month'
      and branch_id = $P{reportParams.branchId}
      and rekening_id IN (
        SELECT
          rekening_id
        FROM
          im_akun_report_detail
        WHERE
          report_id = 'RPT01'
      )
  ) c ON a.rekening_id = c.rekening_id
order by
  a.kode_rekening]]>
	</queryString>

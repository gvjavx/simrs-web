<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.akuntansi.transaksi.budgeting.model.ItAkunBudgetingDetailEntity" dynamic-update="true"
           table="it_akun_budgeting_detail" select-before-update="true" schema="public">

        <id name="noBudgetingDetail">
            <column name="no_budgeting_detail" sql-type="varchar" length="30" not-null="true" />
        </id>

        <property name="noBudgeting">
            <column name="no_budgeting" sql-type="varchar" length="30" not-null="true" />
        </property>
        <property name="divisiId">
            <column name="divisi_id" sql-type="varchar" length="30" not-null="true" />
        </property>
        <property name="nilai">
            <column name="nilai" sql-type="numeric" not-null="false" />
        </property>
        <property name="qty">
            <column name="qty" sql-type="bigint" not-null="false" />
        </property>
        <property name="subTotal">
            <column name="sub_total" sql-type="numeric" not-null="false" />
        </property>
        <property name="tipe">
            <column name="tipe" sql-type="varchar" length="5" not-null="true" />
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
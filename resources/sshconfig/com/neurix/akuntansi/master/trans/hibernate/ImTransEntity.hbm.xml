<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.akuntansi.master.trans.model.ImTransEntity" dynamic-update="true"
           table="im_akun_trans" select-before-update="true" schema="public">

        <id name="transId">
            <column name="trans_id" sql-type="varchar" length="2" not-null="true" />
        </id>

        <property name="transName">
            <column name="trans_name" sql-type="varchar" length="100" not-null="true" />
        </property>
        <property name="tipePembayaran">
            <column name="tipe_pembayaran" sql-type="varchar" length="5" not-null="false" />
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="date" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="date" not-null="false" />
        </property>
        <property name="master">
            <column name="master" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="tipeJurnalId">
            <column name="tipe_jurnal_id" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="flagSumberBaru">
            <column name="flag_sumber_baru" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="flagPengajuanBiaya">
            <column name="flag_pengajuan_biaya" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="isOtomatis">
            <column name="is_otomatis" sql-type="varchar" length="1" not-null="false" />
        </property>


        <set name = "imMappingJurnal" cascade="all">
            <key column = "trans_id"/>
            <one-to-many class="com.neurix.akuntansi.master.mappingJurnal.model.ImMappingJurnalEntity"/>
        </set>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.akuntansi.master.master.model.ImMasterEntity" dynamic-update="true"
           table="im_akun_master" select-before-update="true" schema="public">

        <composite-id  name="primaryKey" class="com.neurix.akuntansi.master.master.model.MasterPK">
            <key-property name="masterId">
                <column name="master_id" sql-type="varchar" length="1" not-null="true" />
            </key-property>
            <key-property name="nomorMaster">
                <column name="nomor_master" sql-type="varchar" length="5" not-null="true"/>
            </key-property>
        </composite-id>

        <property name="akunMasterId">
            <column name="akun_master_id" sql-type="varchar" length="15" not-null="true" />
        </property>
        <property name="nama">
            <column name="nama" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="alamat">
            <column name="alamat1" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="npwp">
            <column name="npwp" sql-type="varchar" length="30" not-null="false" />
        </property>
        <property name="branchId">
            <column name="branch_id" sql-type="varchar" length="5" not-null="false" />
        </property>

        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="date" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="date" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
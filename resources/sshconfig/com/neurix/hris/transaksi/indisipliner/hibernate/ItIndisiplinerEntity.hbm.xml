        <property name="notApprovalNote">
            <column name="not_approval_note" sql-type="varchar" length="1000" not-null="false" />
        </property>
        <property name="closedFlag">
            <column name="closed_flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="closedNote">
            <column name="closed_note" sql-type="varchar" length="1000" not-null="false" />

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.hris.transaksi.payroll.model.ItHrisPayrollHeaderEntity" table="it_hris_payroll_header"
           schema="public" >
        <id name="payrollHeaderId">
            <column name="payroll_header_id" sql-type="varchar(15)" length="15" not-null="true"/>
        </id>
        <property name="tipePayroll">
            <column name="tipe_payroll" sql-type="varchar(2)" length="2" />
        </property>
        <property name="bulan">
            <column name="bulan" sql-type="varchar(2)" length="2" />
        </property>
        <property name="tahun">
            <column name="tahun" sql-type="varchar(4)" length="4" />
        </property>
        <property name="approvalSdmFlag">
            <column name="approval_sdm_flag" sql-type="varchar(1)" length="1" />
        </property>
        <property name="approvalSdmDate">
            <column name="approval_sdm_date" sql-type="timestamp" />
        </property>
        <property name="approvalSdmName">
            <column name="approval_sdm_name" sql-type="varchar(100)" length="100" />
        </property>
        <property name="approvalAksFlag">
            <column name="approval_aks_flag" sql-type="varchar(1)" length="1" />
        </property>
        <property name="approvalAksDate">
            <column name="approval_aks_date" sql-type="timestamp" />
        </property>
        <property name="approvalAksName">
            <column name="approval_aks_name" sql-type="varchar(100)" length="100" />
        </property>
        <property name="keteranganAks">
            <column name="keterangan_aks" sql-type="varchar(1000)" length="1000" />
        </property>

        <property name="noJurnal">
            <column name="no_jurnal" sql-type="varchar(100)" length="100" />
        </property>
        <property name="branchId">
            <column name="branch_id" sql-type="varchar(5)" length="5" />
        </property>

        <property name="createdWho">
            <column name="created_who" sql-type="varchar(100)" length="100" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar(100)" length="100" />
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar(1)" length="1" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar(1)" length="1" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" />
        </property>
    </class>
</hibernate-mapping>
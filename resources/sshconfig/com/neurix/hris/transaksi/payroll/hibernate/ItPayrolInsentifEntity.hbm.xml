<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.hris.transaksi.payroll.model.ItPayrollInsentifEntity" dynamic-update="true"
           table="it_hris_payroll_insentif" select-before-update="true" schema="public">


        <id name="insentifId">
            <column name="insentif_id" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="nip">
            <column name="nip" sql-type="varchar" length="12" not-null="false" />
        </property>
        <property name="payrollId">
            <column name="payroll_id" sql-type="varchar" length="12" not-null="true" />
        </property>
        <property name="jumlahInsentif">
            <column name="jumlah_insentif" sql-type="numeric" length="12" precision="2" not-null="true" />
        </property>
        <property name="jumlahPph">
            <column name="jumlah_pph" sql-type="numeric" length="10" precision="2" not-null="true" />
        </property>

        <property name="gajiGolongan">
            <column name="gaji_golongan" sql-type="numeric" length="12" precision="8" not-null="true" />
        </property>
        <property name="tunjanganUmk">
            <column name="tunjangan_umk" sql-type="numeric" length="12" precision="8" not-null="true" />
        </property>
        <property name="tunjanganStruktural">
            <column name="tunjangan_struktural" sql-type="numeric" length="12" precision="8" not-null="true" />
        </property>
        <property name="tunjanganPeralihan">
            <column name="tunjangan_peralihan" sql-type="numeric" length="12" precision="8" not-null="true" />
        </property>
        <property name="tunjanganJabatanStruktural">
            <column name="tunjangan_jab_struktural" sql-type="numeric" length="12" precision="8" not-null="true" />
        </property>
        <property name="tunjanganStrategis">
            <column name="tunjangan_strategis" sql-type="numeric" length="12" precision="8" not-null="true" />
        </property>
        <property name="jumlahBruto">
            <column name="jumlah_bruto" sql-type="numeric" length="12" precision="8" not-null="true" />
        </property>

        <property name="potPph">
            <column name="pot_pph" sql-type="numeric" length="10" precision="2" not-null="true" />
        </property>
        <property name="potonganinsentifIndividu">
            <column name="potongan_insentif_individu" sql-type="numeric" length="12" precision="2" not-null="true" />
        </property>
        <property name="insentifyangDiterima">
            <column name="insentif_yang_diterima" sql-type="numeric" length="12" precision="2" not-null="true" />
        </property>

        <property name="masaKerja">
            <column name="masa_kerja" sql-type="integer" not-null="true" />
        </property>
        <property name="potonganinsentif">
            <column name="potongan_insentif" sql-type="integer" />
        </property>
        <property name="smkStandart">
            <column name="smk_standart" sql-type="double" />
        </property>
        <property name="smkHuruf">
            <column name="smk_huruf" sql-type="varchar" length="5" />
        </property>
        <property name="smkAngka">
            <column name="smk_angka" sql-type="numeric" length="10" precision="8"  />
        </property>
        <property name="bulanMulai">
            <column name="bulan_mulai" sql-type="integer" not-null="false" />
        </property>
        <property name="bulanSampai">
            <column name="bulan_sampai" sql-type="integer" not-null="false" />
        </property>
        <property name="tahunInsentif">
            <column name="tahun_insentif" sql-type="integer" not-null="false" />
        </property>

        <property name="tahun">
            <column name="tahun" sql-type="varchar" length="4" not-null="false" />
        </property>

        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="date" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="date" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
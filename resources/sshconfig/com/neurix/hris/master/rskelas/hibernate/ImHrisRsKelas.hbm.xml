<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.hris.master.rskelas.model.ImHrisRsKelas" table="im_hris_rs_kelas" dynamic-update="true" select-before-update="true" schema="public">

        <id name="rsKelasId">
            <column name="rs_kelas_id" sql-type="varchar" length="20" not-null="true"/>
        </id>

        <property name="rsId">
            <column name="rs_id" sql-type="varchar" length="5"/>
        </property>
        <property name="kelompokId">
            <column name="kelompok_id" sql-type="varchar" length="5"/>
        </property>
        <property name="golonganId">
            <column name="golongan_id" sql-type="varchar" length="5"/>
        </property>
        <property name="kelas">
            <column name="kelas" sql-type="varchar" length="100"/>
        </property>
        <property name="branchId">
            <column name="branch_id" sql-type="varchar" length="5"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1"/>
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1"/>
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp"/>
        </property>
        <property name="createDateWho">
            <column name="created_who" sql-type="varchar" length="100"/>
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp"/>
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100"/>
        </property>

    </class>
</hibernate-mapping>
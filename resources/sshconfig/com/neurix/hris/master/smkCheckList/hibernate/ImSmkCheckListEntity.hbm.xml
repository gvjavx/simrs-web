<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.hris.master.smkCheckList.model.ImSmkCheckListEntity" dynamic-update="true"
           table="im_hris_smk_check_list" select-before-update="true" schema="public">


        <id name="checkListId">
            <column name="check_list_id" sql-type="varchar" length="3" not-null="true" />
        </id>

        <property name="checkListName">
            <column name="check_list_name" sql-type="varchar" length="100" not-null="true" />
        </property>
        <property name="branchId">
            <column name="branch_id" sql-type="varchar" length="5" not-null="true" />
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="date" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="date" not-null="false" />
        </property>

        <many-to-one name="imBranches" class="com.neurix.authorization.company.model.ImBranches"
                     insert="false" update="false" fetch="select" cascade="all">
            <column name="branch_id"/>
        </many-to-one>

    </class>
</hibernate-mapping>
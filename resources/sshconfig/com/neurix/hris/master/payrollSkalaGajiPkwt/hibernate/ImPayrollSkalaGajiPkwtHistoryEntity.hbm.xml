<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.hris.master.payrollSkalaGajiPkwt.model.ImPayrollSkalaGajiPkwtHistoryEntity" dynamic-update="true"
           table="im_hris_payroll_skala_gaji_pkwt_history" select-before-update="true" schema="public">


        <id name="skalaGajiPkwtIdHistory">
            <column name="skala_gaji_pkwt_id_history" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="skalaGajiPkwtId">
            <column name="skala_gaji_pkwt_id" sql-type="varchar" length="15" not-null="true" />
        </property>



        <property name="gajiPokok">
            <column name="gaji_pokok" sql-type="numeric" length="10" precision="2" not-null="true" />
        </property>
        <property name="santunanKhusus">
            <column name="santunan_khusus" sql-type="numeric" length="10" precision="2" not-null="true" />
        </property>
        <property name="tunjFunsional">
            <column name="tunj_fungsional" sql-type="numeric" length="10" precision="2" not-null="true" />
        </property>
        <property name="tunjtambahan">
            <column name="tunj_tambahan" sql-type="numeric" length="10" precision="2" not-null="true" />
        </property>


        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="date" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="date" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.hris.master.biodata.model.ItTunjLainPegawaiEntity" dynamic-update="true"
           table="it_hris_tunj_lain_pegawai" select-before-update="true" schema="public">


        <id name="tunjLainId">
            <column name="tunj_lain_id" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="nip">
            <column name="nip" sql-type="varchar" length="10" not-null="true" />
        </property>

        <property name="flagTunjSupervisi">
            <column name="flag_tunj_supervisi" sql-type="varchar" length="10" not-null="true" />
        </property>
        <property name="flagTunjLokasi">
            <column name="flag_tunj_lokasi" sql-type="varchar" length="10" not-null="true" />
        </property>

        <!--<property name="flagTunjProfesional">-->
            <!--<column name="flag_tunj_profesional" sql-type="varchar" length="10" not-null="true" />-->
        <!--</property>-->
        <property name="flagTunjSiaga">
            <column name="flag_tunj_siaga" sql-type="varchar" length="10" not-null="true" />
        </property>

        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="date" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="date" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
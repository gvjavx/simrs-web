<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.hris.master.smkKategoriAspekBobot.model.ImSmkKategoriAspekBobotEntity" dynamic-update="true"
           table="im_hris_smk_kategori_aspek_bobot" select-before-update="true" schema="public">


        <id name="kategoriAspekId">
            <column name="kategori_aspek_bobot_id" sql-type="varchar" length="10" not-null="true" />

<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.master.labdetail.model.ImSimrsLabDetailEntity" dynamic-update="true"
           table="im_simrs_lab_detail" select-before-update="true" schema="public">


        <id name="idLabDetail">
            <column name="id_lab_detail" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="idLab">
            <column name="id_lab" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="namaDetailPeriksa">
            <column name="nama_detail_periksa" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="satuan">
            <column name="satuan" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="ketentuanAcuan">
            <column name="keterangan_acuan" sql-type="varchar" length="1000" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>


    </class>
</hibernate-mapping>
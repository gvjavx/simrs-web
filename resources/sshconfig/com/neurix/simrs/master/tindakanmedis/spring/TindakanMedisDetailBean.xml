    <bean id="tindakanMedisDetailBoProxy" class="org.springframework.aop.framework.ProxyFactoryBean"
          p:target-ref="tindakanMedisDetailBo">
        <property name="interceptorNames">
            <list>
                <value>transactionTindakanMedisDetailInterceptor</value>
                <value>hibernateInterceptor</value>
            </list>
        </property>
    </bean>

    <bean id="transactionTindakanMedisDetailInterceptor"
          class="org.springframework.transaction.interceptor.TransactionInterceptor"
          p:transactionManager-ref="transactionManager">
        <property name="transactionAttributes">
            <props>
                <prop key="getByCriteria">readOnly</prop>
                <!--<prop key="saveAdd">PROPAGATION_REQUIRED</prop>-->
            </props>
        </property>
    </bean>

    <bean id="tindakanMedisDetailDao" class="com.neurix.simrs.master.tindakanmedis.dao.TindakanMedisDetailDao"
          p:sessionFactory-ref="sessionFactory" />



</beans>

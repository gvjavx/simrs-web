<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.master.lab.model.ImSimrsLabEntity" dynamic-update="true"
           table="im_simrs_lab" select-before-update="true" schema="public">


        <id name="idLab">
            <column name="id_lab" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="namaLab">
            <column name="nama_lab" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="idOperatorLab">
            <column name="id_operator_lab" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="idDokter">
            <column name="id_dokter" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="idKategoriLab">
            <column name="id_kategori_lab" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="alamat" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>


    </class>
</hibernate-mapping>
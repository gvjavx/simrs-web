<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.master.lab.model.ImSimrsLabEntity" dynamic-update="true"
           table="im_simrs_lab" select-before-update="true" schema="public">

        <id name="idLab">
            <column name="id_lab" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="namaLab">
            <column name="nama_lab"/>
        </property>
        <property name="flag">
            <column name="flag"/>
        </property>
        <property name="action">
            <column name="action"/>
        </property>
        <property name="createdDate">
            <column name="created_date"/>
        </property>
        <property name="createdWho">
            <column name="created_who"/>
        </property>
        <property name="lastUpdate">
            <column name="last_update"/>
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who"/>
        </property>
        <property name="isCatatan">
            <column name="is_catatan"/>
        </property>
        <property name="isPaket">
            <column name="is_paket"/>
        </property>
        <property name="isAsesmen">
            <column name="is_asesmen"/>
        </property>
    </class>
</hibernate-mapping>
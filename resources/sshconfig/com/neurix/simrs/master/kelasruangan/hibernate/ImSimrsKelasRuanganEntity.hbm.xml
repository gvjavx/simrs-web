<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.master.kelasruangan.model.ImSimrsKelasRuanganEntity" dynamic-update="true"
           table="im_simrs_kelas_ruangan" select-before-update="true" schema="public">


        <id name="idKelasRuangan">
            <column name="id_kelas_ruangan" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="namaKelasRuangan">
            <column name="nama_kelas_ruangan" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="kodering">
            <column name="kodering" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="divisiId">
            <column name="divisi_id" sql-type="varchar" length="30" not-null="false" />
        </property>
        <property name="kategori">
            <column name="kategori"/>
        </property>

    </class>
</hibernate-mapping>
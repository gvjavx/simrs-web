<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.simrs.master.license.model.ImVersionZebraEntity" dynamic-update="true"
           table="im_version_zebra" select-before-update="true" schema="public">

        <id name="idVersion">
            <column name="id_version" sql-type="varchar" length="20" not-null="true" />
        </id>

        <property name="versionName">
            <column name="version_name" sql-type="varchar" length="20"/>
        </property>

        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>

        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>

        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="50" not-null="false" />
        </property>

        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="50" not-null="false" />
        </property>

        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>

        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>

        <property name="description">
            <column name="description" sql-type="text" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
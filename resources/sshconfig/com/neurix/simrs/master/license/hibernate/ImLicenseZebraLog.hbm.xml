<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.simrs.master.license.model.ImLicenseZebraLogEntity" dynamic-update="true"
           table="im_license_zebra_log" select-before-update="true" schema="public">

        <id name="licenseLogId">
            <column name="license_log_id" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="licenseId">
            <column name="license_id" sql-type="varchar" length="10" />
        </property>

        <property name="licenseKey">
            <column name="license_key" sql-type="varchar" length="100"/>
        </property>

        <property name="deviceId">
            <column name="device_id" sql-type="varchar" length="100"/>
        </property>

        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>

        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>

        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="50" not-null="false" />
        </property>

        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="50" not-null="false" />
        </property>

        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>

        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.periksaradiologi.model.ItSimrsPeriksaRadiologiEntity" dynamic-update="true"
           table="it_simrs_periksa_radiologi" select-before-update="true" schema="public">


        <id name="idPeriksaRadiologi">
            <column name="id_periksa_radiologi" sql-type="varchar" length="30" not-null="true" />
        </id>

        <property name="idDokterRadiologi">
            <column name="id_dokter_radiologi" sql-type="varchar" length="30" not-null="true"/>
        </property>
        <property name="pemeriksaan">
            <column name="pemeriksaan" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="kesimpulan">
            <column name="kesimpulan" sql-type="varchar" length="1000" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="alamat" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>


    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.diagnosarawat.model.ItSimrsDiagnosaRawatEntity" dynamic-update="true"
           table="it_simrs_diagnosa_rawat" select-before-update="true" schema="public">


        <id name="idDiagnosaRawat">
            <column name="id_diagnosa_rawat" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="idDiagnosa">
            <column name="id_diagnosa" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="idDetailCheckup">
            <column name="id_detail_checkup" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="keteranganDiagnosa">
            <column name="keterangan_diagnosa" sql-type="varchar" length="1000" not-null="false"/>
        </property>
        <property name="jenisDiagnosa">
            <column name="jenis_diagnosa" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="tipe">
            <column name="tipe" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>


    </class>
</hibernate-mapping>
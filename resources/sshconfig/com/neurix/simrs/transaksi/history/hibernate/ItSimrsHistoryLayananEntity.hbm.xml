<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.history.model.ItSimrsHistoryPelayananEntity" dynamic-update="true"
           table="it_simrs_history_pelayanan" select-before-update="true" schema="public">


        <id name="idHistoryPelayanan">
            <column name="id_history_pelayanan" sql-type="varchar" length="30" not-null="true" />
        </id>
        <property name="idDetailCheckup">
            <column name="id_detail_checkup" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="idRuangan">
            <column name="id_ruangan" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="namaRuangan">
            <column name="nama_ruangan" sql-type="varchar" length="100" />
        </property>
        <property name="noRuangan">
            <column name="no_ruangan" sql-type="varchar" length="100" />
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="alamat" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>


    </class>
</hibernate-mapping>
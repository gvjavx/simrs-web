<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <!--<bean id="jenisObatAction" class="com.neurix.simrs.master.jenisobat.action.JenisObatAction"-->
          <!--p:jenisObatBoProxy-ref="jenisObatBoProxy"/>-->

    <!--<bean id="jenisObatBo" class="com.neurix.simrs.transaksi.periksalab.bo.impl."-->
          <!--p:jenisObatDao-ref="jenisObatDao"/>-->

    <bean id="jenisObatBoProxy" class="org.springframework.aop.framework.ProxyFactoryBean"
          p:target-ref="jenisObatBo">
        <property name="interceptorNames">
            <list>
                <value>transactionPeriksaLabDetailInterceptor</value>
                <value>hibernateInterceptor</value>
            </list>
        </property>
    </bean>

    <bean id="transactionPeriksaLabDetailInterceptor"
          class="org.springframework.transaction.interceptor.TransactionInterceptor"
          p:transactionManager-ref="transactionManager">
        <property name="transactionAttributes">
            <props>
                <prop key="getByCriteria">readOnly</prop>
            </props>
        </property>
    </bean>

    <bean id="periksaLabDetailDao" class="com.neurix.simrs.transaksi.periksalab.dao.PeriksaLabDetailDao"
          p:sessionFactory-ref="sessionFactory" />

</beans>
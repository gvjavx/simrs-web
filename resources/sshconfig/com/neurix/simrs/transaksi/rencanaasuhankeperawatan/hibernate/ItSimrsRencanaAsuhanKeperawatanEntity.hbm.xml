<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.rencanaasuhankeperawatan.model.ItSimrsRencanaAsuhanKeperawatanEntity" dynamic-update="true"
           table="it_simrs_rencana_asuhan_keperawatan" select-before-update="true" schema="public">

        <id name="idRencanaAsuhanKeperawatan">
            <column name="id_rencana_asuhan_keperawatan" sql-type="varchar" length="30" not-null="true" />
        </id>

        <property name="idDetailCheckup">
            <column name="id_detail_checkup" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="waktu">
            <column name="waktu" sql-type="timestamp" not-null="false"/>
        </property>
        <property name="diagnosa">
            <column name="diagnosa" sql-type="text" not-null="false"/>
        </property>
        <property name="tujuan">
            <column name="tujuan" sql-type="text" not-null="false"/>
        </property>
        <property name="intervensi">
            <column name="intervensi" sql-type="text" not-null="false"/>
        </property>
        <property name="ttdPerawat">
            <column name="ttd_perawat" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="keterangan">
            <column name="keterangan" sql-type="varchar" length="1000" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>

    </class>
</hibernate-mapping>
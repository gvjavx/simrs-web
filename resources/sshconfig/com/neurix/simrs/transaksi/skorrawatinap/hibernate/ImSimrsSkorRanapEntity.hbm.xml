<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.skorrawatinap.model.ImSimrsSkorRanapEntity" dynamic-update="true"
           table="im_simrs_skor_rawat_inap" select-before-update="true" schema="public">

        <id name="idSkor">
            <column name="id_skor" sql-type="varchar" length="30" not-null="true" />
        </id>
        <property name="namaSkor">
            <column name="nama_skor" sql-type="varchar" length="1000" not-null="false"/>
        </property>
        <property name="skor">
            <column name="skor" sql-type="integer" not-null="false"/>
        </property>
        <property name="idParameter">
            <column name="id_parameter" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>
    </class>
</hibernate-mapping>
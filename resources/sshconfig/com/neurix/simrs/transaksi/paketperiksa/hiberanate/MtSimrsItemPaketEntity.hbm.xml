<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.paketperiksa.model.MtSimrsItemPaketEntity" dynamic-update="true"
           table="mt_simrs_item_paket_periksa" select-before-update="true" schema="public">


        <id name="idItemPaket">
            <column name="id_item_paket" sql-type="varchar" length="30" not-null="true" />
        </id>

        <property name="idPaket">
            <column name="id_paket" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="idItem">
            <column name="id_item" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="idKategoriItem">
            <column name="id_kategori_item" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="jenisItem">
            <column name="jenis_item" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="harga">
            <column name="harga" sql-type="bigint" not-null="false" />
        </property>


    </class>
</hibernate-mapping>
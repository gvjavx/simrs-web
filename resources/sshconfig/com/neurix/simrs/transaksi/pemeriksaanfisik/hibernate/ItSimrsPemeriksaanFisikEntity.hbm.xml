<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neurix.simrs.transaksi.pemeriksaanfisik.model.ItSimrsPemeriksaanFisikEntity" dynamic-update="true"
           table="it_simrs_pemeriksaan_fisik" select-before-update="true" schema="public">


        <id name="id">
            <column name="id" sql-type="varchar" length="30" not-null="true" />
        </id>
        <property name="noCheckup">
            <column name="no_checkup" sql-type="varchar" length="30" not-null="false"/>
        </property>
        <property name="kepala">
            <column name="kepala" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="mata">
            <column name="mata" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="leher">
            <column name="leher" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="thorak">
            <column name="thorak" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="thorakChor">
            <column name="thorak_cor" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="thorakPulmo">
            <column name="thorak_pulmo" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="abdoman">
            <column name="abdoman" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="extrimitas">
            <column name="extrimitas" sql-type="varchar" length="100" not-null="false"/>
        </property>
        <property name="tinggiBadan">
            <column name="tinggi_badan" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="beratBadan">
            <column name="berat_badan" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="nadi">
            <column name="nadi" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="respirationRate">
            <column name="respiration_rate" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="tekananDarah">
            <column name="tekanan_darah" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="suhu">
            <column name="suhu" sql-type="varchar" length="10" not-null="false"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="timestamp" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="100" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="timestamp" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="100" not-null="false" />
        </property>


    </class>
</hibernate-mapping>
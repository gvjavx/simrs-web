<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.neurix.hris.transaksi.payroll.model.ImPayrollBpjsEntity" dynamic-update="true"
           table="im_hris_payroll_bpjs" select-before-update="true" schema="public">


        <id name="bpjsId">
            <column name="bpjs_id" sql-type="varchar" length="10" not-null="true" />
        </id>

        <property name="bpjsKesehatanPersen">
            <column name="bpjs_kesehatan_persen" sql-type="numeric" length="5" not-null="true" />
        </property>
        <property name="bpjsPensiunPersen">
            <column name="bpjs_pensiun_persen" sql-type="numeric" length="5" not-null="true" />
        </property>
        <property name="bpjsJhtPersen">
            <column name="bpjs_jht_persen" sql-type="numeric" length="5" not-null="true" />
        </property>


        <!--//tambahanirfan
        //bpjs Kesehatan-->
        <property name="iuranBpjsKsKaryPersen">
            <column name="iuran_bpjs_ks_kary_persen" sql-type="numeric" length="10" not-null="true" />
        </property>
        <property name="iuranBpjsKsPersPersen">
            <column name="iuran_bpjs_ks_pers_persen" sql-type="numeric" length="10" not-null="true" />
        </property>
        <property name="minBpjsKs">
            <column name="min_bpjs_ks" sql-type="numeric" length="10" not-null="true" />
        </property>
        <property name="maxBpjsKs">
            <column name="max_bpjs_ks" sql-type="numeric" length="10" not-null="true" />
        </property>

        <!--//bpjs tenaga kerja-->
        <property name="iuranBpjsTkKaryPersen">
            <column name="iuran_bpjs_tk_kary_persen" sql-type="numeric" length="10" not-null="true" />
        </property>
        <property name="iuranBpjsTkPersPersen">
            <column name="iuran_bpjs_tk_pers_persen" sql-type="numeric" length="10" not-null="true" />
        </property>
        <property name="minBpjsTk">
            <column name="min_bpjs_tk" sql-type="numeric" length="10" not-null="true" />
        </property>
        <property name="maxBpjsTk">
            <column name="max_bpjs_tk" sql-type="numeric" length="10" not-null="true" />
        </property>

        <property name="flag">
            <column name="flag" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="action">
            <column name="action" sql-type="varchar" length="1" not-null="false" />
        </property>
        <property name="createdWho">
            <column name="created_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="lastUpdateWho">
            <column name="last_update_who" sql-type="varchar" length="20" not-null="false" />
        </property>
        <property name="createdDate">
            <column name="created_date" sql-type="date" not-null="false" />
        </property>
        <property name="lastUpdate">
            <column name="last_update" sql-type="date" not-null="false" />
        </property>

    </class>
</hibernate-mapping>